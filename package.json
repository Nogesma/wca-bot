{
  "name": "wca-bot",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prod": "yarn run init && yarn run start",
    "init": "node init.js",
    "start": "node index.js",
    "dev": "node --require dotenv/config init.js && nodemon --ignore data/ --exec node --require dotenv/config --inspect index.js",
    "codestyle": "prettier -c **/*.js *.json"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "country-code-emoji": "^2.3.0",
    "cron": "^2.4.4",
    "dayjs": "^1.11.13",
    "discord.js": "^14.18.0",
    "graphql": "^16.10.0",
    "graphql-request": "^5.2.0",
    "node-fetch": "^3.3.2",
    "ramda": "^0.28.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/node": "^20.17.28",
    "dotenv": "^16.4.7",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3"
  },
  "packageManager": "yarn@4.2.2"
}
